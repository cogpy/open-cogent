{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Open-Agent Cloud",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["oa", "@afk/server", "dev"]
    },
    {
      "name": "Lanuch Open-Agent Web",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["oa", "@afk/web", "dev"]
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug Open-Agent Web",
      "url": "http://localhost:8080"
    }
  ]
}
